<%- include('includes/header.ejs') %>
<h1 class="mt-5 mb-3">Welcome to Image Processing API Udacity</h1>
<p>Available Images:</p>
<ul>
  <% for(image of images){ %>
  <li>
    <a href="?filename=<%= trimExtension(image); %>"
      ><%= trimExtension(image); %></a
    >
  </li>
  <% } %>
</ul>

<p>
  To get each image for any defult size add <b>width=50&height=50</b> after the
  filename
</p>
<h6>Example</h6>
<p>
  <li>
    <a
      href="http://localhost:4000/images?filename=encenadaport&width=50&height=50"
      >encenadaport</a
    >
  </li>
  <li>
    <a href="http://localhost:4000/images?filename=fjord&width=50&height=50"
      >fjord</a
    >
  </li>
  <li>
    <a
      href="http://localhost:4000/images?filename=icelandwaterfall&width=50&height=50"
      >icelandwaterfall</a
    >
  </li>
  <li>
    <a
      href="http://localhost:4000/images?filename=palmtunnel&width=50&height=50"
      >palmtunnel</a
    >
  </li>
  <li>
    <a
      href="http://localhost:4000/images?filename=santamonica&width=50&height=50"
      >santamonica</a
    >
  </li>
</p>

